  <li>
    <strong>Suite:</strong>
    <%= @user.suite %>
  </li>

  <li>
    <strong>City:</strong>
    <%= @user.city %>
  </li>

  <li>
    <strong>State:</strong>
    <%= @user.state %>
  </li>

  <li>
    <strong>Zip code:</strong>
    <%= @user.zip_code %>
