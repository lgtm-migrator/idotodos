<h2>Show user</h2>

<ul>

  <li>
    <strong>First name:</strong>
    <%= @user.first_name %>
  </li>

  <li>
    <strong>Last name:</strong>
    <%= @user.last_name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Middle name:</strong>
    <%= @user.middle_name %>
  </li>

  <li>
    <strong>Gender:</strong>
    <%= @user.gender %>
